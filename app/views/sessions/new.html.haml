%h1 Log In
#log-in
  .form-horizontal.col-md-7{role: 'form'}
    = form_tag sessions_path do
      .field
        = label_tag :email
        = email_field_tag :email, params[:email]
        .field
        = label_tag :password
        = password_field_tag :password, params[:password]
      .actions
        =submit_tag "Log in"
